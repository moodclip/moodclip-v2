[
  {
    "origin": ["*"],
    "method": ["GET", "HEAD", "PUT", "POST", "DELETE"],
    "responseHeader": [
      "Content-Type",
      "Content-Range",
      "Content-Disposition",
      "Content-Length",
      "ETag",
      "Tus-Resumable",
      "Tus-Version",
      "Tus-Extension",
      "Tus-Max-Size",
      "Upload-Concat",
      "Upload-Defer-Length",
      "Upload-Length",
      "Upload-Metadata",
      "Upload-Offset"
    ],
    "maxAgeSeconds": 3600
  }
]
